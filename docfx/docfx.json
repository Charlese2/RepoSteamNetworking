{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../RepoSteamNetworking/",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
    
      "filter": "./filterConfig.yml",
      "enumSortOrder": "declaringOrder"
    }
  ],  
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "examples/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "RepoSteamNetworking",
      "_appTitle": "RepoSteamNetworking",
      "_enableSearch": true,
      "pdf": true
    },
    "xref": [
      "https://rageagainstthepixel.github.io/UnityDocfxXrefMaps/2022.3/xrefmap.yml"
    ]
  }
}